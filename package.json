    "unplugin-vue-components": "^0.16.0",
    "vue-cli-plugin-windicss": "~1.0.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "**/*.{vue}": "npm run lint:style",
    "**/*.{css,sass,scss}": "npm run lint:css",
    "**/*.{vue,js,jsx,ts,tsx}": "npm run lint:fix"
  }
}
